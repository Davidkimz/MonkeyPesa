<div class="col-md-10">
    <label class="form-group" for="displayName">Name</label>
    <div class="input-group">
      <div class="input-group-prepend">
        <span class="input-group-text fa fa-user"></span>
      </div>
      <input type="text" class="form-control" name="displayName" id="displayName" oninput="checkRegisterInput('register')" placeholder="Name">
    </div>
  </div>

  <div class="col-md-10">
    <label class="form-group" for="email">Email</label>
    <div class="input-group">
      <div class="input-group-prepend">
        <span class="input-group-text fas fa-envelope"></span>
      </div>
      <input type="email" class="form-control" name="email" id="email" oninput="checkRegisterInput('register')" placeholder="Email" autocomplete="username">
    </div>
  </div>

  <div class="col-md-10">
    <label class="form-group" for="website">Website</label>
    <div class="input-group">
      <div class="input-group-prepend">
        <span class="input-group-text fas fa-address-card"></span>
      </div>
      <input type="text" class="form-control" name="website" id="website" placeholder="Website">
    </div>
  </div>

  <div class="input-group col-md-10">
    <label class="form-group" for="password">Password</label>
    <div class="input-group">
      <div class="input-group-prepend">
        <span class="input-group-text fas fa-lock"></span>
      </div>
      <input type="password" class="form-control" name="password" id="password" oninput="checkRegisterInput('register')" placeholder="Password" autocomplete="current-password">
    </div>
  </div>

  <div class="input-group col-md-10">
    <label class="form-group" for="passwordCheck">Repeat Password</label>
    <div class="input-group">
      <div class="input-group-prepend">
        <span class="input-group-text fas fa-lock"></span>
      </div>
      <input type="password" class="form-control" name="passwordCheck" id="passwordCheck" oninput="checkRegisterInput('register')" placeholder="Repeat Password">
    </div>
  </div>